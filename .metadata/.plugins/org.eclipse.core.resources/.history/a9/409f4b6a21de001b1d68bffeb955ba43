package inheritance5;
class A {
	public A() {
		System.out.println("持失切A");
	}
	public A(int x) {
		System.out.println("古移痕呪持失切A");
	}
}
class B extends A {
	public B() {
		System.out.println("持失切B");
	}
	public B(int x) {
		System.out.println("古鯵痕呪持失切B");
	}
}

public class ConstructorEX03 {

	public static void main(String[] args) {

		B b;
		b = new B(5);
	}

}
